{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
  ],
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "fileMatch": [
        "(^|/)\\.chezmoidata\\.toml$"
      ],
      "matchStrings": [
        "# renovate:(?: packageName=(?<packageName>[^\\s]+?))?(?: extractVersion=(?<extractVersion>[^\\s]+?))? +.*\\s+[A-Za-z0-9_]+ ?= ?[\"']?(?<currentValue>.+?)[\"']?\\s"
      ]
    }
  ]
}
